{
   "description": "Solve the equation x^2 = 2^0.5 * x",
   "request_list": [
   {
      "request": "What is the first step?",
      "type": "all",
      "responses": ["Bring all terms to the left side of the equation"]
   },
   {
      "request": "What is the new equation?",
      "type": "expression",
      "splitter" : "=",
      "responses": 
      ["x^2 - 2^0.5 * x = 0"]
   },
   {
      "request": "What is the next step?",
      "type": "all",
      "responses": ["Factorize left side of equation"]
   },
   {
      "request": "What is the new equation?",
      "type": "expression",
      "splitter" : "=",
      "responses": 
      ["x * (x - 2^0.5) = 0"]
   },
   {
      "request": "What is the next step?",
      "type": "all",
      "responses": ["Set each factor to zero"]
   },
   {
      "request": "What are the new equations?",
      "type": "expression",
      "splitter" : "=",
      "responses": 
      ["x = 0", "x-2^0.5 = 0"]
   },
   {
      "request": "What are the solutions?",
      "type": "expression",
      "splitter": "",
      "responses": 
      ["0", "2^0.5"]
   }
   ]
}
